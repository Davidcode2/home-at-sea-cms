{
  "kind": "collectionType",
  "collectionName": "itineraries",
  "info": {
    "singularName": "itinerary",
    "pluralName": "itineraries",
    "displayName": "Itinerary",
    "description": "Ship itinerary/route"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "yearRound": {
      "type": "boolean",
      "default": true
    },
    "ship": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::ship.ship",
      "inversedBy": "itineraries"
    },
    "stops": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::itinerary-stop.itinerary-stop",
      "mappedBy": "itinerary"
    }
  }
}
